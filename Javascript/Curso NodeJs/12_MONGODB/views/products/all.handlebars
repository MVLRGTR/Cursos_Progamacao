<h1>Produtos</h1>
<div class="products">
    {{#each ProductsAll}}
        <div class="product">
            <img src="{{this.ImageUrl}}" alt="{{this.name}}">
            <h3>{{this.name}}</h3>
            <p>R$:{{this.price}}</p>
            <div class="actions">
                <a href="/products/{{this._id}}">Detalhes</a>
            </div>
        </div>
    {{/each}}
</div>